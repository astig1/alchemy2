<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">

<plugin id="org.alchemy.create.evolveShapes" version="1.0.0">
    
    <requires> 
        <import plugin-id="org.alchemy.core"/> 
    </requires> 
    
    <runtime>
        <library id="evolveShapes" path="/" type="code">
            <export prefix="*" /> 
        </library>
        <library type="resources" path="data/" id="data" />
    </runtime>
    
    <extension plugin-id="org.alchemy.core" point-id="Create" id="evolveShapes"> 
        <parameter id="class" value="org.alchemy.create.EvolveShapes"/> 
        <parameter id="name" value="Evolve Shapes"/> 
        <parameter id="description" value="Evolve shapes with a genetic algorithm" />
        <parameter id="icon" value="icon.png" />
    </extension>
    
</plugin>